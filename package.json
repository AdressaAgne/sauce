{
  "name": "awesomesouce",
  "version": "4.0.0",
  "description": "an awesome library filled with souce",
  "main": "index.js",
  "scripts": {
    "dev": "npx spack"
  },
  "author": "Agne Ã˜degaard",
  "license": "ISC",
  "devDependencies": {
    "@fastify/static": "^5.0.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.194",
    "chalk": "^4.0.1",
    "fastify": "^3.29.0",
    "glob": "^8.0.3",
    "node-sass": "^7.0.1"
  },
  "swc": {
    "sourceMaps": true,
    "minify": true,
    "jsc": {
      "target": "es5",
      "parser": {
        "syntax": "ecmascript",
        "jsx": true,
        "dynamicImport": false,
        "privateMethod": true,
        "functionBind": false,
        "exportDefaultFrom": false,
        "exportNamespaceFrom": false,
        "decorators": false,
        "decoratorsBeforeExport": false,
        "topLevelAwait": false,
        "importMeta": false,
        "preserveAllComments": false
      },
      "transform": {
        "react": {
          "pragma": "vNode",
          "pragmaFrag": "vFragment",
          "throwIfNamespace": true,
          "development": false,
          "useBuiltins": false
        },
        "optimizer": {
          "globals": {
            "vars": {
              "__DEBUG__": "true"
            }
          }
        }
      }
    },
    "$schema": "http://json.schemastore.org/swcrc",
    "module": {
      "type": "commonjs",
      "strict": false,
      "strictMode": true,
      "lazy": false,
      "noInterop": false
    }
  }
}
